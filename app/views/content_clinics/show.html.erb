<div class="panel panel-default">
<div class="panel-heading"> 
Wyedytowano dane</div>
<div class="panel-body"> 

<table class="table table-condensed">
<thead>
<tr>
<th>Nazwa kliniki</th>
<th>Adres kliniki</th>
<th>Kod ZIP kliniki</th>
<th>Miejscowość</th>
</tr>
</thead>
<tbody>
  <td><%= @content_clinic.name_clinic %></td>

  <td><%= @content_clinic.adress_clinic %></td>

  <td><%= @content_clinic.zip_code_clinic %></td>

  <td><%= @content_clinic.city_linic %></td>
</tr> 
</tbody>
</table>

</div>
<div class="panel-footer">
<%= link_to 'Edytuj', edit_content_clinic_path(@content_clinic), class:"btn btn-success"  %> 
<%= link_to 'Wróć', content_clinics_path, class:"btn btn-info"  %>
</div>